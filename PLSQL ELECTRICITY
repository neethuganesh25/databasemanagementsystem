DECLARE 
CURSOR CUR IS SELECT * FROM EB;
BEGIN
FOR I IN CUR
LOOP
IF I.UNITS<=100 THEN
UPDATE EB SET CHARGES=I.UNITS*1 WHERE
EBNO=I.EBNO;
ELSIF I.UNITS>100 AND I.UNITS<=200 THEN
UPDATE EB SET CHARGES=I.UNITS*2 WHERE EBNO=I.EBNO;
ELSE
UPDATE EB SET CHARGES=I.UNITS*3 WHERE EBNO=I.EBNO;
END IF;
END LOOP;
END;
